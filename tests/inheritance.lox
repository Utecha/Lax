class Human {
    init(name, age, sex, height, weight) {
        self.name = name;
        self.age = age;
        self.sex = sex;
        self.height = height;
        self.weight = weight;
    }

    info() {
        print "Humans Info:";
        print self.name;
        print self.age;
        print self.sex;
        print self.height;
        print self.weight;
    }

    walk() {
        print self.name + " is walking.";
    }

    run() {
        print self.name + " is running!";
    }
}

var homosapien = Human("Chandler", 27, "Male", 69, 130);
homosapien.info();
homosapien.walk();
homosapien.run();

class Leader < Human {
    init(name, age, sex, height, weight, role) {
        super.init(name, age, sex, height, weight);
        self.role = role;
    }

    lead() {
        print self.name + " says 'Charge!'.";
    }
}

var leader = Leader("The President", 40, "Male", 80, 175, "President");
leader.info();
leader.walk();
leader.run();
leader.lead();
print leader.role;
